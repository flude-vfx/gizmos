Group {
 name glowy
 inputs 2
 tile_color 0xff5400ff
 addUserKnob {20 fxT_glowy}
 addUserKnob {26 divline0 l "" +STARTLINE}
 addUserKnob {20 ReformatSettings l "Reformat Settings" t "Unfold this group to set your preferred output format. Default format is set to the input format." n 1}
 ReformatSettings 0
 addUserKnob {6 disableReformat l "activate reformat options" t "By default the gizmo inherits the input format.\n\nCheck this box to bypass the input format, and instead activate the reformat options specified in the below dropdown-list." +STARTLINE}
 addUserKnob {41 format l "output format" T GizmoReformat.format}
 addUserKnob {41 pbb l "preserve bounding box" t "reformat to the output format, and perserver the boundingbox." T GizmoReformat.pbb}
 addUserKnob {20 endGroup n -1}
 addUserKnob {26 divline1 l "" +STARTLINE}
 addUserKnob {4 glowOperation l "glow operation" t "The operation type that is used to merge the glow with the input." M {plus screen under ""}}
 addUserKnob {26 divline2 l "" +STARTLINE}
 addUserKnob {41 tint l "color tint:" T Gain_and_Tint.tint}
 addUserKnob {26 divline3 l "" +STARTLINE}
 addUserKnob {7 tolerance t "Addjust the tolerance of the glow."}
 tolerance 0.7
 addUserKnob {7 falloff t "Addjust the falloff of the glow." R 0 4}
 falloff 2.5
 addUserKnob {41 gain t "Addjust the gain of the glow." T Gain_and_Tint.gain}
 addUserKnob {26 divline4 l "" +STARTLINE}
 addUserKnob {7 glow_size l "glow radius" t "Gradually adds exponential levels of glow to create a more realistic falloff." R 0 5}
 glow_size 1
 addUserKnob {41 saturation t "Addjust the saturation of the glow." T glow_saturation.saturation}
 addUserKnob {26 divline5 l "" +STARTLINE}
 addUserKnob {7 anamorphicSlider l "        anamorphic stretch" t "Drag the slider to add an anamorphic look to the glow.\n\nNon-anamorphic ratio is automatically set to 0. Drag the slider to increase the anamorphic stretch.\n________________________\n\nAnanorphic stretch: Streches the glow in X.\nAddjust the glow radius to addjust the Y-stretch." -STARTLINE R 0 150}
 addUserKnob {26 ""}
 addUserKnob {6 effectOnly l "effect only" t "Check this box to show only the glow effect." -STARTLINE}
 addUserKnob {6 preserveAlpha t "By default, the alpha generated by the glow is removed and the original alpha is preserved. The glow is only added to the RGB channels.\n\nCheck this box to preserve the alpha generated by the glow operation." -STARTLINE}
 addUserKnob {6 invertMask t "Invert the mask input." -STARTLINE}
 addUserKnob {41 white_clamp l "white clamp" T Clamp.white_clamp}
 addUserKnob {41 black_clamp l "black clamp" -STARTLINE T Clamp.black_clamp}
 addUserKnob {26 divline6 l "" +STARTLINE}
 addUserKnob {7 glowOpacity l "glow opacity" t "Set the opacity of the glow."}
 glowOpacity 1
 addUserKnob {26 divline7 l "" +STARTLINE}
 addUserKnob {26 copyright l "" +STARTLINE T "<font color='#434343'>// fxT_glowy v1.5 Â© Tor Andreassen - www.fxtor.net\n</font>"}
}
